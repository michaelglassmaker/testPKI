    <fields>
        <fullName>SMAX_PS_AutoAdjust_InvoiceNotReq__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Indicates that this line was Auto Adjusted to 100% Discount due to the Invoice Not Required checkbox.</description>
        <externalId>false</externalId>
        <inlineHelpText>Indicates that this line was Auto Adjusted to 100% Discount due to the Invoice Not Required checkbox.</inlineHelpText>
        <label>Auto Adjusted for Invoice Not Required</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>SMAX_PS_CalculatedLinePrice__c</fullName>
        <externalId>false</externalId>
        <formula>/* Start with the Replacement Base Price if not blank.. */ 
BLANKVALUE(SMAX_PS_ReplacementBasePrice__c, 
/* OR the Actual Price - Contract Discount */ 
(SVMXC__Price__c * 
(1.0 - (BLANKVALUE(SMAX_PS_ContractPercentDiscount__c, 0.00) / 100)) )) 
/* Multiply by Qty */ 
* SVMXC__Quantity__c 

/* Next Apply the Service Percent Adjustment */ 
* IF (ISBLANK(SMAX_PS_ServicePercentAdjustment__c), 
1.0, 
IF (ISPICKVAL(SMAX_PS_ServicePercentAdjustmentType__c, &quot;Discount&quot;), 
1.0 - (SMAX_PS_ServicePercentAdjustment__c / 100), 
1.0 + (SMAX_PS_ServicePercentAdjustment__c / 100) 
) 
) 

/* Next Apply the Service Amount Adjustment */ 
+ IF (ISBLANK(SMAX_PS_ServiceAmountAdjustment__c), 
0.0, 
IF (ISPICKVAL(SMAX_PS_ServiceAmountAdjustmentType__c, &quot;Discount&quot;), 
(-1.0 * SMAX_PS_ServiceAmountAdjustment__c), 
SMAX_PS_ServiceAmountAdjustment__c 
) 
)</formula>
        <label>Calculated Line Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
