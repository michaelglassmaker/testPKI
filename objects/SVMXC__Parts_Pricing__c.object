<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>SVMX_PS_ServicePlan_Or_Contract</fullName>
        <active>true</active>
        <errorConditionFormula>IF(LEN(SVMXC__Service_Contract__r.Id) &gt; 0, IF(LEN( SVMXC__Service_Plan__r.Id ) &gt; 0, TRUE, FALSE), IF(LEN(SVMXC__Service_Plan__r.Id) &gt; 0, FALSE,TRUE))</errorConditionFormula>
        <errorMessage>Parts Pricing must be associated with either Service Plan or Service Contract, but not both.</errorMessage>
    </validationRules>
</CustomObject>
