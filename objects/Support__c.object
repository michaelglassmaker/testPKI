    <validationRules>
        <fullName>GBL_GBL_Support_newuser_fields_populated</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND( ISPICKVAL( Problem_type__c,&apos;New User Request&apos;), ISBLANK( New_User_Email_ID__c ) ) , 
AND(NOT( ISBLANK( New_User_Email_ID__c )) , NOT(ISPICKVAL( Problem_type__c,&apos;New User Request&apos;))) )</errorConditionFormula>
        <errorMessage>IF &apos;problem type&apos; is New User Request, ensure &apos;New User Creation Details&apos; section is populated. Otherwise leave it blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GBL_GBL_Support_reactivatefields_popultd</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND( ISPICKVAL( Problem_type__c,&apos;User Reactivation&apos;), ISBLANK( Username_to_Reactivate__c ) ) , 
AND(NOT( ISBLANK( Username_to_Reactivate__c )) , NOT(ISPICKVAL( Problem_type__c,&apos;User Reactivation&apos;))) )</errorConditionFormula>
        <errorMessage>IF &apos;problem type&apos; is User Reactivation, ensure &apos;Username to reactivate&apos; field is populated. For all other problem types, &apos;Username to reactivate&apos;  field must be left blank.</errorMessage>
    </validationRules>
