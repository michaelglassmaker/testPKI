<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This will store mapping for each object to indicate which field is used when emailing contact. Here specify API of a field on the object which are lookup to contacts.</description>
    <enableFeeds>false</enableFeeds>
    <label>Email Contact Field Mapping Settings</label>
    <visibility>Public</visibility>
    <fields>
        <fullName>SVMX_PS_ES_Object_API_Name__c</fullName>
        <description>This field carries the Object API Name</description>
        <externalId>false</externalId>
        <inlineHelpText>This field carries the Object API Name</inlineHelpText>
        <label>Object API Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SVMX_PS_ES_Contact_Field_API_Name__c</fullName>
        <description>This field carries the Contact Field API Name. It is necessary to put a field which is lookup of contact. No other field should be put here</description>
        <externalId>false</externalId>
        <inlineHelpText>This field carries the Contact Field API Name. It is necessary to put a field which is lookup of contact. No other field should be put here</inlineHelpText>
        <label>Contact Field API Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
</CustomObject>