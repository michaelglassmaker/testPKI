    <webLinks>
        <fullName>Clone_with_Line_Items</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Clone with Line Items</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)}

var objID = &quot;{!ServiceContract.Id}&quot;;

var finishedLocation;

if (window.parent.location.href==&quot;/ui/desktop/DesktopPage&quot;) { 
finishedLocation = &apos;/{!ServiceContract.Id}&amp;isdtp=mn&apos;; 
} else { 
finishedLocation = &apos;/&apos; + objID; 
}; 

if (objID.length != 0) {

// Make a synchronous call to Apex Web service method
var result = sforce.apex.execute(
&quot;ServiceContractButton&quot;, // Class
&quot;cloneServiceContract&quot;, // method
{serviceContractId: objID});

// createAttempt() returns new Service Contract page
if (result != &apos;&apos;) {
window.location.href = &apos;/&apos; + result;
}

} else if (objID.length == 0) {

alert(&quot;No Service Contract has been cloned.&quot;);
}</url>
    </webLinks>
