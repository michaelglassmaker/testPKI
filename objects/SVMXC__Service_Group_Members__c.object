        <description>Checks if the logged user is the technician - used in mass accept</description>
        <externalId>false</externalId>
        <formula>IF( SVMXC__Salesforce_User__c = $User.Id || 
$Profile.Name = &quot;System Administrator&quot; || 
$Profile.Name = &quot;SVMX PS - Service Manager - Platform&quot;|| 
$Profile.Name = &quot;SVMX PS - Customer Care&quot;, 
1, 
0 
)</formula>
