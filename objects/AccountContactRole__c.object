    <validationRules>
        <fullName>Cannot_Edit_Fields</fullName>
        <active>true</active>
        <description>Cannot edit SAP Customer Number unless you have a specific profile.</description>
        <errorConditionFormula>AND($Setup.Rules_Execution__c.Run__c, 
 OR(ISCHANGED(RelatedAccount__c),
  ISCHANGED( Related_Contacts__c)), 
 NOT($Profile.Name = &apos;Application Integration profiles&apos;))</errorConditionFormula>
        <errorMessage>You cannot edit fields unless you have a specific profile</errorMessage>
