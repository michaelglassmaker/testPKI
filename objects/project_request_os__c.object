        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/connection.js&quot;)} 
sforce.connection.query(&quot;SELECT Id,Request_Status__c FROM project_request_os__c where Id= &apos;{!project_request_os__c.Id}&apos;&quot;,{onSuccess: handleSuccess,onFailure: handleFailure}); 
function handleSuccess(queryresult) 
{ 
try 
{ 
records = queryresult.getArray(&quot;records&quot;); 
records[0].Id=&apos;{!project_request_os__c.Id}&apos;; 
records[0].Request_Status__c =&apos;Submitted&apos;; 
var uprec = sforce.connection.update(records); 
if (uprec[0].getBoolean(&quot;success&quot;)) 
{ 
window.location.reload(); 
} 
else 
{ 
alert(uprec[0]); 
} 
} 
catch(error) 
{ 
alert(error); 
} 
} 
function handleFailure(error) 
{ 
alert(error); 
}</url>
