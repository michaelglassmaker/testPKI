    <validationRules>
        <fullName>INF_Start_and_End_date_mandatory</fullName>
        <active>false</active>
        <description>if Product name contains licenseStart Date and End Date is mandatory</description>
        <errorConditionFormula>AND(
	OR(
		ISBLANK(Start_Date_INF__c),
		ISBLANK( End_Date_INF__c)
	),
	OR(
		CONTAINS(PricebookEntry.Product2.Name,&apos;Subscription&apos;),CONTAINS(PricebookEntry.Product2.Name,&apos;Maintenance&apos;),CONTAINS(PricebookEntry.Product2.Name,&apos;Term&apos;)
	),
	Product2.Business_Unit__c == &apos;INF&apos; 
)</errorConditionFormula>
        <errorDisplayField>Covered_Asset__c</errorDisplayField>
        <errorMessage>Enter Start and End Date for subscription, maintenance and term products.</errorMessage>
    </validationRules>
