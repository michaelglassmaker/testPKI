    <validationRules>
        <fullName>Cannot_Create_Quote_on_Closed_Oppty</fullName>
        <active>false</active>
        <errorConditionFormula>ISNEW()
&amp;&amp;
ISPICKVAL(SBQQ__Opportunity2__r.StageName, &apos;Closed Won&apos;)</errorConditionFormula>
        <errorMessage>You cannot create a quote on a closed opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Updates_After_Pending_and_Approved</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
AND(PRIORVALUE(SBQQ__NetAmount__c)!=SBQQ__NetAmount__c,Approval_Locked__c!=TRUE, 
OR(ISPICKVAL(ApprovalStatus__c,&apos;Pending&apos;),ISPICKVAL(ApprovalStatus__c,&apos;Approved&apos;)) 
),TRUE,FALSE)</errorConditionFormula>
        <errorMessage>To make additional changes, you must “Recall” the Quote approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Updates_after_Quote_Locked</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
AND(PRIORVALUE(SBQQ__NetAmount__c)!=SBQQ__NetAmount__c,Approval_Locked__c==TRUE),TRUE,FALSE)</errorConditionFormula>
        <errorMessage>Quote is Locked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Notes_cannot_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
AND(
  OR(ISCHANGED(SBQQ__Notes__c),ISCHANGED(Terms_Notes__c)),
     ISPICKVAL(ApprovalStatus__c,&apos;Approved&apos;)
   ),TRUE,FALSE)</errorConditionFormula>
        <errorMessage>To make additional changes, you must “Recall” the Quote approval</errorMessage>
    </validationRules>
