    <fields>
        <fullName>SMAX_PS_CalculatedLinePrice__c</fullName>
        <description>2018-05-17 Maples Edited to move * QTy to end and paren around 1st 2 blocks to calculate properly
2018-07-30 VanLoon - Reverting the changes Maples made for SVMXCFG-683</description>
        <externalId>false</externalId>
        <formula>/* Start with the Replacement Base Price if not blank.. */ 
BLANKVALUE(SMAX_PS_ReplacementBasePrice__c, 
/* OR the Actual Price - Contract Discount */ 
(SVMXC__Actual_Price2__c * 
(1.0 - (BLANKVALUE(SMAX_PS_ContractPercentDiscount__c, 0.00) / 100)) )) 
/* Multiply by Qty */ 
* SVMXC__Actual_Quantity2__c 

/* Next Apply the Service Percent Adjustment */ 
* IF (ISBLANK(SMAX_PS_ServicePercentAdjustment__c), 
1.0, 
IF (ISPICKVAL(SMAX_PS_ServicePercentAdjustmentType__c, &quot;Discount&quot;), 
1.0 - (SMAX_PS_ServicePercentAdjustment__c / 100), 
1.0 + (SMAX_PS_ServicePercentAdjustment__c / 100) 
) 
) 

/* Next Apply the Service Amount Adjustment */ 
+ IF (ISBLANK(SMAX_PS_ServiceAmountAdjustment__c), 
0.0, 
IF (ISPICKVAL(SMAX_PS_ServiceAmountAdjustmentType__c, &quot;Discount&quot;), 
(-1.0 * SMAX_PS_ServiceAmountAdjustment__c), 
SMAX_PS_ServiceAmountAdjustment__c 
) 
)</formula>
        <label>Calculated Line Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>SMAX_PS_Calculated_Discount__c</fullName>
        <externalId>false</externalId>
        <formula>IF (ISPICKVAL(SMAX_PS_ServicePercentAdjustmentType__c, &quot;Discount&quot;), 
TEXT(SMAX_PS_ServicePercentAdjustment__c) +&quot;%&quot;, 
IF (ISPICKVAL(SMAX_PS_ServiceAmountAdjustmentType__c, &quot;Discount&quot;), 
SVMXC__Service_Order__r.SMAX_PS_Currency_Symbol__c +&quot; &quot;+ TEXT(SMAX_PS_ServiceAmountAdjustment__c), 
NULL 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calculated Discount</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SMAX_PS_Calculated_Line_Discount__c</fullName>
        <externalId>false</externalId>
        <formula>IF (SVMXC__Total_Line_Price__c - SMAX_PS_CalculatedLinePrice__c &gt;0,
     SVMXC__Total_Line_Price__c - SMAX_PS_CalculatedLinePrice__c,
     NULL
     )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calculated Line Discount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>SMAX_PS_Calculated_Unit_Price__c</fullName>
        <description>2018-07-30 VanLoon - Added / Qty for SVMXCFG-683</description>
        <externalId>false</externalId>
        <formula>/* Start with the Replacement Base Price if not blank and not 0.. */
(IF ( AND( NOT(ISBLANK(SMAX_PS_ReplacementBasePrice__c)), 
                SMAX_PS_ReplacementBasePrice__c &lt;&gt; 0),
       SMAX_PS_ReplacementBasePrice__c,
/* OR the Actual Price - Contract Discount */
       (SVMXC__Actual_Price2__c *
       (1.0 - (BLANKVALUE(SMAX_PS_ContractPercentDiscount__c, 0.00) / 100)) )
      )
 
/* Next Apply the Service Amount Adjustment */
 + IF (ISBLANK(SMAX_PS_ServiceAmountAdjustment__c),
         0.0,
         IF (ISPICKVAL(SMAX_PS_ServiceAmountAdjustmentType__c, &quot;Discount&quot;),
             (-1.0 * SMAX_PS_ServiceAmountAdjustment__c),
             SMAX_PS_ServiceAmountAdjustment__c
             ) / SVMXC__Actual_Quantity2__c
         )
)
/* Next Apply the Service Percent Adjustment */
 * IF (ISBLANK(SMAX_PS_ServicePercentAdjustment__c),
        1.0,
        IF (ISPICKVAL(SMAX_PS_ServicePercentAdjustmentType__c, &quot;Discount&quot;),
             1.0 - (SMAX_PS_ServicePercentAdjustment__c / 100),
             1.0 + (SMAX_PS_ServicePercentAdjustment__c / 100)
            )
        )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calculated Unit Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
